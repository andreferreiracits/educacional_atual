<script type="text/javascript">
    
    $(document).ready(function(){

            $("#icDestaque").click(function(){
                if($("#icon_destaque").hasClass("btn_destaque")){
                    $("#icon_destaque").removeClass("btn_destaque");
                    $("#icon_destaque").addClass("btn_destaque_off");

                    $("#icon_destaque").removeClass("bounceInRight");
                     $("#icon_destaque").addClass("slideInLeft");
                }
                else{
                    $("#icon_destaque").removeClass("btn_destaque_off");
                    $("#icon_destaque").addClass("btn_destaque"); 

                    $("#icon_destaque").removeClass("slideInLeft");
                    $("#icon_destaque").addClass("slideInRight");
                    
                }
            });
        });

        $(document).ready(function(){
            $("#icDestaque").click(function(){
                if($("#colunaDireitaFixa").hasClass("showColunaDireita")){
                    $("#colunaDireitaFixa").removeClass("showColunaDireita");
                    $("#colunaDireitaFixa").addClass("hideColunaDireita");

                    $("#colunaDireitaFixa").removeClass("slideInRight");
                     $("#colunaDireitaFixa").addClass("slideInLeft");
                }
                else{
                    $("#colunaDireitaFixa").removeClass("hideColunaDireita");
                    $("#colunaDireitaFixa").addClass("showColunaDireita"); 

                    $("#colunaDireitaFixa").removeClass("slideInLeft");
                    $("#colunaDireitaFixa").addClass("slideInRight");  
                }
            });
            setTimeout(function(){ 

                if($("#colunaDireitaFixa").hasClass("showColunaDireita")){
                    $("#colunaDireitaFixa").removeClass("showColunaDireita");
                    $("#colunaDireitaFixa").addClass("hideColunaDireita");

                    $("#colunaDireitaFixa").removeClass("slideInRight");
                     $("#colunaDireitaFixa").addClass("slideInLeft");
                }
                if($("#icon_destaque").hasClass("btn_destaque")){
                    $("#icon_destaque").removeClass("btn_destaque");
                    $("#icon_destaque").addClass("btn_destaque_off");

                    $("#icon_destaque").removeClass("bounceInRight");
                     $("#icon_destaque").addClass("slideInLeft");
                }


            }, 50000);
        });

        
    </script>


