        <script type="text/javascript">
            
          
            console.log("abcd");

            function removeClassList(){
                $("div#icons").removeAttr('class');
            }

            function fechaFiltroAtividade() {
                $('#filtroAtv ul').removeClass('open');
            }

            var intTipoAtividadeGlobal = 0;
            var tempoFechaFiltroAtividade;

            jQuery(function ($) {

             
                

                //------ menu dropdown da lista de atividades
                $('#filtroAtv ul').on('click',function(){
                    $(this).toggleClass('open');

                    $(this).hasClass('open') ? tempoFechaFiltroAtividade = setTimeout(fechaFiltroAtividade,5000): clearTimeout(tempoFechaFiltroAtividade);
                });

                $('#filtroAtv ul li').on('click',function(){
                    var elemento = $(this).clone(true);
                    var id = elemento.attr("id");
                    var temp = intTipoAtividadeGlobal;
                    switch(id) 
                    {
                        case "3" : intTipoAtividadeGlobal = 3;
                            break;
                        
                        case "2" : intTipoAtividadeGlobal = 2;
                            break;

                        case "1" : intTipoAtividadeGlobal = 1;
                            break;

                        default: intTipoAtividadeGlobal = 0;
                            break;
                    }

                    if (intTipoAtividadeGlobal == temp) return; // mesma opção selecionada, não chamar o back end desnecessariamente

                    var filtro = $('#filtroAtv ul');
                    if (filtro.children().length > 4)
                        filtro.children(':first').remove();
                    else
                        filtro.children(':first').removeClass('filho_selecionado').css('display', '');
                    
                    filtro.prepend(elemento.css('display', 'block').addClass('filho_selecionado'));

                    carregaBoxAtividades();

                    setTimeout(function(){
                        //elemento.css('display', 'block').addClass('filho_selecionado');
                    }, 1000);
                });

                $(".form-control-icon").on('click',function(){
                    if( $(this).hasClass('true') ){
                        $(this).find("ul.list-dropdown").hide();
                        $(this).removeClass('true');
                    }
                    else{
                        $(this).addClass('true');
                        $(this).find("ul.list-dropdown").show();
                    }                                   
                });


                $(".form-control-user ul li").on('click',function(){

                    var id = $(this).attr("id");

                    if( id == "3" ){
                        botao = "avaliacao-t";
                        removeClassList();
                        $("span#icons").addClass(botao);

                    }else if( id == "2" ){
                        botao = "tarefa-t";
                        removeClassList();
                        $("span#icons").addClass(botao);
                    }
                    else if( id == "1" ){
                        botao = "caminho-t";
                        removeClassList();
                        $("span#icons").addClass(botao);
                    }else{
                        removeClassList();
                    }

                    $(".form-control-user").find('div').text($(this).text());
                });

                $(".form-control-user").on('click',function(){
                    $(this).find("ul.list-dropdown-user").toggle(400);
                });


                $("body").on("click", function (e) {
                    if ($(e.target).closest(".form-control-icon, ul.list-dropdown").length)
                        return;

                    $("ul.list-dropdown").hide();

                });                         
                //------ menu dropdown da lista de atividades

            });


            if (_projeto != "grupo")
            {
                carregaBoxAtividades();

                jQuery(function ($) {
                    $.ajax({
                        url: "/AVA/adminAVA/Avisos/VerificaAvisoPorData",
                        async: true,
                        data: { 'ts': new Date().getTime() },
                        contentType: "application/x-www-form-urlencoded; charset=iso-8859-1",
                        success: function (data) {
                            if (data.length > 0) {
                                $("#avisos_conteudo").text(data);
                                $("#avisos").show();
