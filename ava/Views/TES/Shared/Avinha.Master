        <script type="text/javascript">

            var intTipoAtividadeGlobal = 0;
            function removeClassList() {
                $("div#icons").removeAttr('class');
            }

            jQuery(function ($) {

                //   fillZEform();
                  
                //------ menu dropdown da lista de atividades
                $(".form-control-icon ul li").on('click', function () {

                    var id = $(this).attr("id");

                    if (id == "3") {
                        botao = "avaliacao-t";
                        removeClassList();
                        $("div#icons").addClass(botao);
                        intTipoAtividadeGlobal = 3;
                        carregaBoxAtividades();

                    } else if (id == "2") {
                        botao = "tarefa-t";
                        removeClassList();
                        $("div#icons").addClass(botao);
                        intTipoAtividadeGlobal = 2;
                        carregaBoxAtividades();
                    }
                    else if (id == "1") {
                        botao = "caminho-t";
                        removeClassList();
                        $("div#icons").addClass(botao);
                        intTipoAtividadeGlobal = 1;
                        carregaBoxAtividades();
                    } else {
                        botao = "todas-t";
                        removeClassList();
                        $("div#icons").addClass(botao);
                        intTipoAtividadeGlobal = 0;
                        carregaBoxAtividades();
                    }

                    $('.t-conteudo').text($(this).text());
                });


                $(".form-control-icon").on('click', function () {
                    if ($(this).hasClass('true')) {
                        $(this).find("ul.list-dropdown").hide();
                        $(this).removeClass('true');
                    }
                    else {
                        $(this).addClass('true');
                        $(this).find("ul.list-dropdown").show();
                    }
                });


                $(".form-control-user ul li").on('click', function () {

                    var id = $(this).attr("id");

                    if (id == "3") {
                        botao = "avaliacao-t";
                        removeClassList();
                        $("span#icons").addClass(botao);

                    } else if (id == "2") {
                        botao = "tarefa-t";
                        removeClassList();
                        $("span#icons").addClass(botao);
                    }
                    else if (id == "1") {
                        botao = "caminho-t";
                        removeClassList();
                        $("span#icons").addClass(botao);
                    } else {
                        removeClassList();
                    }

                    $(".form-control-user").find('div').text($(this).text());
                });

                $(".form-control-user").on('click', function () {
                    $(this).find("ul.list-dropdown-user").toggle(400);
                });


                $("body").on("click", function (e) {
                    if ($(e.target).closest(".form-control-icon, ul.list-dropdown").length)
                        return;

                    $("ul.list-dropdown").hide();

                });
                //------ menu dropdown da lista de atividades

            });


            // Renan Daré - 12/04/2013
            
          
            // BUSCA A IDADE DO USUÁRIO DE ACORDO COM O IDSERIE DELE
            var idSerieUsuarioLogado = '1';

            jQuery(function ($) {
                carregaBoxAtividades();  
                var idadeUsuario            = '7';

                $(".idade_aluno .komika").empty();
                $.ajax({
                    url: "/AVA/Mural/Home/BuscaIdserieUsuario",
                    async: true,
                    contentType: "application/x-www-form-urlencoded; charset=iso-8859-1",
                    success: function (data) {

                        idSerieUsuarioLogado = parseInt(data);

                        switch (idSerieUsuarioLogado) {
                            case 12:
                                idadeUsuario = '3';
                                break;
                            case 13:
                                idadeUsuario = '4';
                                break;
                            case 14:
                                idadeUsuario = '5';
                                break;
                            case 15:
                                idadeUsuario = '6';
                                break;
                            case 16:
                                idadeUsuario = '6';
                                break;
                            case 27:
                                idadeUsuario = '6';
                                break;
                            case 9:
                                idadeUsuario = '6';
                                break;
                            case 1:
                                idadeUsuario = '7';
                                break;
                            default:
                                idadeUsuario = '7';
                        }
                        $(".ver_todas").attr('href', '/AVA/Mural/Home/Jogos/' + idadeUsuario)
                        $(".idade_aluno .komika").html(idadeUsuario + ' ANOS');
                        if (idadeUsuario == 3) {
                            $('.botao_baixo').addClass('inativo');
                        } else {
                            $('.botao_baixo').removeClass('inativo');
                        }
                        if (idadeUsuario == 7) {
                            $('.botao_cima').addClass('inativo');
                        } else {
                            $('.botao_cima').removeClass('inativo');
                        }
                        BuscarMaisVistos(idSerieUsuarioLogado);
                    }
                });
            });

            //function verificaSeFilhoTemTurma() {  
            jQuery(function ($) {
                $.ajax({
                    type: "POST",
                    url: "/AVA/Caminhos/Home/VerificaAlunoTemTurma",
                    data: { 'strLogin': idUsuarioPublico },
                    success: function (msg) {
                        if (msg != "ok") {
                            $('#sLista_atividade .boxFooter').hide();
                            $('#sLista_atividade').find('header .form-box').hide();
                            $('#sLista_atividade').find('header #t-info').hide();
                            $('#sLista_atividade').find('header').append('<div><h2>&nbsp;&nbsp;&nbsp;Atividades</h2></div>');
                            $('#ava_barralateral-direita_avinha').find('header h2').css('margin-top', '5px')
                        }
                    }
                });
            });
            //}
            jQuery(function ($) {
                var qtipOptionsTop = {
                    style: { classes: 'qtip-dark' },
                    position: {
                        my: 'bottom center',  // Position my top left...
                        at: 'top left' // at the bottom right of...
                    }
                };

                $('#t-info.tooltip_title_top[title!=""]').qtip(qtipOptionsTop);
            });

            // Carregar os jogos de acordo com a série do usuário logado na barra da direita.
            jQuery(function ($) {

                $("#ava_barralateral-direita_avinha .idade_aluno ul").empty();
                $.ajax({
                    url: "/AVA/Mural/Home/BuscaJogosRandomicosPorSerie",
                    async: true,
                    data: { 'idSerie': idSerieUsuarioLogado },
                    dataType: "json",
                    contentType: "application/x-www-form-urlencoded; charset=iso-8859-1",
                    success: function (data) {

                        if (data.Total > 0) {
                            $(data.materiais).each(function (i, e) {

                                // Validação para caso seja uma url com javascript, pra abrir na mesma janela, se não, em nova janela.
                                var target = '';
                                if (e.strLink.toLowerCase().indexOf("javascript") > -1) {
                                    target = '_self';
                                } else {
                                    target = '_blank';
                                }

                                if (e.bolNovo == 1) {
                                      var html = "<li><a target='" + target + "' href='" + e.strLink + "' class='tooltip_title' title='" + e.strJogo + "'><img src='" + e.strThumb + "' width='116' height='81' alt=''></a><span></span></li>";

                                } else {
                                     var html = "<li><a target='" + target + "' href='" + e.strLink + "' class='tooltip_title' title='" + e.strJogo + "'><img src='" + e.strThumb + "' width='116' height='81' alt=''></a></li>";
                                }
                                
                                $("#ava_barralateral-direita_avinha .idade_aluno ul").append(html);

                            });

                        }
                    }
                });
            });
            var altura = screen.height;
            var largura = screen.width;

            // Validação dos botões para aumentar e diminuir a data dos jogos a serem listados
            jQuery(function ($) {
                $(".botao_cima").click(function () {
                    var intIdadeAtual = parseInt($('.idade_aluno .komika').html().charAt(0));
                    var novoIdSerie = '';


                    if (intIdadeAtual == 7) {
