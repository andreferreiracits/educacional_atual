        <script type="text/javascript">

            jQuery(function ($) {  

            // fillZEform();

                                 
                if (!bolCPPuro) {
                    $.ajax({
                        url: "/AVA/Barras/Home/VerificaAcessoListaAtividadesGrupo",
                        data: {'strLinkGrupo': idUsuarioPublico},
                        async: true,
                        success: function (retorno) {
                        
                            if (retorno == "aluno") {
                                $("#sLista_atividade").show();
                                $("#lista_atividades").html("<img src='/AVA/StaticContent/Common/img/perfil/carregando.gif' border='0' />").show();
                                $.ajax({
                                    url: "/AVA/Caminhos/Home/CriarCaminhosAlunoGrupo",
                                    async: true,
                                    success: function () {
                                    
                                        $.ajax({
                                            url: "/AVA/Barras/Home/ListaAtividadesGrupo",
                                            data: { 'strLinkGrupo': idUsuarioPublico },
                                            async: true,
                                            contentType: "application/x-www-form-urlencoded; charset=UTF-8",
                                            success: function (data) {
                                                $("#lista_atividades").html(data);
                                                $(".tooltip_title").tooltip({
			                                        offset: [0, 0],
			                                        opacity: 0.9,
		                                        });                                                    
                                                $(".content").mCustomScrollbar();
                                            },
                                            error: function (data) {
                                                $("#lista_atividades").html("erro ao listar atividades do aluno.");
                                            }
                                        });                                       

                                    },
                                    error: function (data) {
                                        $("#lista_atividades").html("erro ao criar caminhos do aluno.");
                                    }
                                });

                            }
                            else if(retorno == "professor")
                            {
                                $("#sVisualizarAgendamento").show();
                                $("#lista_agendamento").html("<img src='/AVA/StaticContent/Common/img/perfil/carregando.gif' border='0' />").show();
                            
                                $.ajax({
                                    url: "/AVA/Barras/Home/ListaAtividadesGrupo",
                                    data: { 'strLinkGrupo': idUsuarioPublico },
                                    async: true,
                                    contentType: "application/x-www-form-urlencoded; charset=UTF-8",
                                    success: function (data) {
                                        $("#lista_agendamento").html(data);   
                                    
                                        $(".tooltip_title").tooltip({
			                                offset: [0, 0],
			                                opacity: 0.9,
		                                });  
                                                                                        
                                        $(".content").mCustomScrollbar();                                 
                                    },
                                    error: function (data) {
                                        $("#lista_agendamento").html("erro ao listar atividades do professor.");
                                    }
                                });                                      

                                
                            }

                        },
                        error: function (data) {
                            $("#lista_atividades").html("erro ao listar atividades do grupo.");
                        }
                    });
                }
            });  
               
           
            //se for grupo esconde algumas funcionalidades e mostra funcionalidades do grupo                
            $("#sProcuraPessoas, #sEducadores, #sTurma, .seguidos_seguidores").css("display", "none");
            $("#sMediadoresGrupo, #sParticipantesGrupo, #sAssuntosGrupo").css("display", "");                
            
            jQuery(function ($) {
                $("#fecharavisoNavegadorDesatualizado")
                .click(function (e) {
                    e.preventDefault();
                    $($(this).parent()).hide();
                })
                .css("cursor", "pointer");
            });

            jQuery(function ($) {

                var idStrGrupo = $("#strIdLinkPermanente").val();

                $.ajax({
                    url: "/AVA/Grupo/Home/BarraSuperior",
                    async: true,
                    data: {
                        id: idStrGrupo
                    },
                    contentType: "application/x-www-form-urlencoded; charset=iso-8859-1",
                    success: function (data) {
                        $(".barra_superior_grupos").html(data);
                        if (parseInt($('body').data('bolclube')) == 1) {

                            $.ajax({
                                dataType: "json",
                                url: "/cp/clube/funcoesGrupoAVA/jsonClubes.asp",
                                async: true,
                                data: {
                                    idEdicao: <%= ViewData["idEdicao"] %>
                                },
                                success: function (data) {
                                    var j = data;
                                    var htmlMenu = '';

                                        for (i = 0; i < j.clube.menu.length; i++) {
                                            var classeCSS = "";
                                            var htmlseta = "";
                                            if ((j.clube.menu[i].strDescricao !== undefined && j.clube.menu[i].strDescricao != null) || (j.clube.menu[i].subitem !== undefined && j.clube.menu[i].subitem != null)) {
                                                classeCSS = "abraChampz";
                                                if(j.clube.menu[i].subitem !== undefined && j.clube.menu[i].subitem != null){
                                                    htmlseta = " <span></span>";
                                                }
                                            }
                                            htmlMenu += '<div class="' + classeCSS + '">';

                                            if (j.clube.menu[i].strTipo !== undefined && j.clube.menu[i].strTipo != null && j.clube.menu[i].strLink !== undefined && j.clube.menu[i].strLink != null) {
                                                if (j.clube.menu[i].strTipo == "fancybox") {
                                                    htmlMenu += '<a href="' + j.clube.menu[i].strLink + '" class="abreFancybox fancybox.iframe">' + j.clube.menu[i].strTitulo + htmlseta +'</a> ';
                                                } else {
                                                    htmlMenu += '<a href="' + j.clube.menu[i].strLink + '">' + j.clube.menu[i].strTitulo + htmlseta +'</a> ';
                                                }
                                            } else {
                                                htmlMenu += '<a href="javascript:;">' + j.clube.menu[i].strTitulo + htmlseta +'</a> ';
                                            }

                                            if (j.clube.menu[i].strDescricao !== undefined && j.clube.menu[i].strDescricao != null) {
                                                htmlMenu += '	<div class="combo_topo sobre_combo"> '
                                                         + '		<div class="seta_top"></div> '
                                                         + '		<div> '
                                                         + '		    <p>' + j.clube.menu[i].strDescricao + '</p> '
                                                         + '        </div> '
                                                         + '    </div> ';
                                            }

                                            if (j.clube.menu[i].subitem !== undefined && j.clube.menu[i].subitem != null) {
